{
  "name": "${containerWorkspaceFolderBasename}",
  "build": {
    "dockerfile": "Dockerfile",
  },
  "runArgs": [
    "--name",
    "${containerWorkspaceFolderBasename}",
  ],
  "extensions": [
    "dbaeumer.vscode-eslint",
    "eamodio.gitlens",
    "mhutchie.git-graph",
    "gruntfuggly.todo-tree",
    "ionutvmi.path-autocomplete",
    "vsls-contrib.gistfs",
    "loiane.ionic-extension-pack"
  ]
}